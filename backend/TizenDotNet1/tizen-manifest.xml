<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.tizen.example.TizenDotNet1" version="1.0.0" api-version="6.5" xmlns="http://tizen.org/ns/packages">
    <profile name="common" />
    <service-application appid="org.tizen.example.TizenDotNet1" exec="TizenDotNet1.dll" multiple="false" nodisplay="true" taskmanage="false" api-version="9" type="dotnet">
        <label>TizenDotNet1</label>
        <icon>TizenDotNet1.png</icon>
        <app-control>
            <operation name="http://org.example.TizenDotNet1/dlnascan" />
            <uri name="http://org.example.TizenDotNet1" />
            <mime name="application/json" />
            <visibility name="both" />
        </app-control>
        <background-category value="media" />
        <background-category value="download" />
        <background-category value="background-network" />
        <splash-screens />
    </service-application>
    <shortcut-list />
	<privileges>
		<privilege>http://tizen.org/privilege/application.launch</privilege>
	</privileges>
    <dependencies />
    <provides-appdefined-privileges />
</manifest>
